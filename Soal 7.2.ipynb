   "source": [
    "import math\n",
    "\n",
    "n = int(input(\"Masukkan nilai n: \"))\n",
    "\n",
    "for i in range(n, 0, -1):\n",
    "    faktorial = math.factorial(i)  \n",
    "    deret = \" \".join(map(str, range(i, 0, -1)))  \n",
    "    print(f\"{faktorial} {deret}\")  \n"
   ]
