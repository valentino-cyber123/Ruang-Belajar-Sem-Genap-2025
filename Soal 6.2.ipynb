   "source": [
    "def ganjil(bawah, atas):\n",
    "    if bawah < atas:\n",
    "        return [i for i in range(bawah + 1, atas, 2) if i % 2 == 1]\n",
    "    else:\n",
    "        return [i for i in range(bawah, atas - 1, -2) if i % 2 == 1]\n",
    "\n",
    "# Contoh penggunaan\n",
    "print(ganjil(10, 30))  \n",
    "print(ganjil(97, 82)) "
   ]
